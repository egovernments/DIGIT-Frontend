# Production Environment Variables
NODE_ENV=production
REACT_APP_PROXY_URL=https://unified.digit.org
REACT_APP_USER_TYPE=EMPLOYEE

# API Keys (Production - Use your production keys)
REACT_APP_GMAPS_API_KEY=your-production-google-maps-key

# Debug Settings
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Performance
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Asset Optimization
IMAGE_INLINE_SIZE_LIMIT=10000
CSS_MODULE_LOCAL_IDENT_NAME=[hash:base64:5]