# Proxy Server Configuration
# Port for the proxy server to listen on
PROXY_PORT=3002

# Elasticsearch Configuration
# URL of the Elasticsearch cluster
ELASTICSEARCH_URL=https://localhost:9200

# Elasticsearch Authentication
# Username for Elasticsearch basic auth
ES_USERNAME=elastic
# Password for Elasticsearch basic auth  
ES_PASSWORD=changeme

# Kibana Configuration
# URL of the Kibana instance
KIBANA_URL=https://localhost:5601

# Development/Production Settings
# Set to 'development' for detailed logging, 'production' for minimal logging
NODE_ENV=development

# CORS Configuration (optional)
# Comma-separated list of allowed origins for CORS
# CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# Request Timeout (optional)
# Timeout for requests to Elasticsearch/Kibana in milliseconds
# REQUEST_TIMEOUT=60000

# SSL/TLS Configuration (optional)
# Set to 'false' to disable SSL certificate verification (development only)
# REJECT_UNAUTHORIZED=false


# CORS Configuration (optional)
# Server URL that is allowed to access the proxy (hostname and port)
ALLOWED_SERVER_URL=https://yourdomain.com:8080

# Comma-separated list of allowed context paths (app contexts)
# Examples: console,workbench,admin,health
ALLOWED_CONTEXT_PATHS=console,workbench