# Environment Variables Example
# Copy this file to .env.development or .env.production and configure

# Basic Configuration
NODE_ENV=development
REACT_APP_PROXY_URL=https://your-backend-url.com
REACT_APP_USER_TYPE=EMPLOYEE
PORT=3000

# API Keys
REACT_APP_GMAPS_API_KEY=your-google-maps-api-key

# Feature Flags
REACT_APP_ENABLE_TELEMETRY=false
REACT_APP_ENABLE_ANALYTICS=false

# Debug Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Build Settings
GENERATE_SOURCEMAP=true
FAST_REFRESH=true

# API Endpoints (Optional - Override defaults)
REACT_APP_MDMS_CONTEXT_PATH=mdms-v2
REACT_APP_HRMS_CONTEXT_PATH=health-hrms
REACT_APP_PROJECT_CONTEXT_PATH=health-project