  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development",
    "started":"cd micro-ui-internals && yarn build && cd ../ && yarn start:dev",
    "start:core":"cd micro-ui-internals && yarn build:core && cd ../ && yarn start:dev",
    "start:dev": "webpack serve --config webpack.dev.js --mode development",
    "build": "cross-env GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true build",
    "build:prepare": "./build.sh",
    "build:libraries": "cd micro-ui-internals && yarn build",
    "build:prod": "webpack --mode development",
    "build:webpack": "yarn build:libraries && cd .. && ls && cd ./web && ls && yarn build:prod",
    "clean": "rm -rf node_modules && cd micro-ui-internals && yarn clean"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
