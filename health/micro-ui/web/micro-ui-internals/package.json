    "start:dev": "cd example && yarn start",
    "start:script": "./scripts/create.sh",
    "build": "run-s build:**",
    "buildDev": "run-s buildDev:**",
    "build:css": "cd packages/css && yarn build",
    "buildDev:css": "cd packages/css && yarn build:dev",
    "build:campaign": "cd packages/modules/campaign-manager && yarn build",
    "buildDev:campaign": "cd packages/modules/campaign-manager && yarn build:dev",
    "cleancmd": "find . \\( -name 'node_modules' -o -name 'dist' \\) -type d -prune -exec rm -rf '{}' +",
