                                "nextDoseId": {
                                    "order": 4,
                                    "fallback": 1,
                                    "condition": {
                                        "if": {
                                            "left": "{{cycle}}",
                                            "right": "{{currentRunningCycle}}",
                                            "operator": "equals"
                                        },
                                        "else": 1,
                                        "then": {
                                            "if": {
                                                "left": {
                                                    "value": "{{dose}}",
