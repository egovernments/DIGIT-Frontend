                          },
                          {
                            "when": "{{item.id}} == {{contextData.0.nextDoseId}}",
                            "value": "REGISTRATION_CURRENT_DOSE_STATUS_TOBE_ADMINISTERED"
                          }
                        ]
                      }
                    },
                    {
                      "header": "COMPLETED_ON",
                      "isActive": true,
                      "cellValue": "{{fn:getTaskCompletionDate(item.id, contextData.0.nextCycleIndex)}}"
                    }
                  ]
                },
                "type": "template",
                "format": "table",
                "fieldName": "deliveryTable"
              },
              {
                "type": "template",
                "format": "expandable",
                "children": [
