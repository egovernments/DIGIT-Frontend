{
  "name": "@digit-ui/digit-ui-react-components",
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "example": "cd example && npm run start",
    "build-webpack": "webpack --mode=production --node-env=production",
    "build":"yarn build-webpack",
    "publish": "webpack --config webpack.config.js && npm publish --tag mfe",
    "build-old": "microbundle-crl --compress --no-sourcemap --format cjs",
    "starts": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o dist-storybook",
    "deploy-storybook": "npm run build-storybook && surge --project dist-storybook --domain react-components-$npm_package_version.surge.sh",
    "start": "webpack serve --config webpack.config.js"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "gh-pages": "2.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "microbundle-crl": "0.13.11",
    "react-scripts": "^4.0.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "5.7.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@digit-ui/digit-ui-svg-components":"1.0.1",
    "@googlemaps/js-api-loader": "1.13.10",
    "@hookform/resolvers": "1.3.7",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "babel-loader": "8.1.0",
    "json-schema-to-yup": "^1.8.8",
    "react-date-range": "1.3.0",
    "date-fns":"2.28.0",
    "react-hook-form": "6.15.8",
    "react-i18next": "11.16.2",
    "react-table": "7.7.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.6.1",
    "react-router-dom": "5.3.0",
    "react-joyride": "2.5.5",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "i18next": "19.9.2",
    "i18next-react-postprocessor": "3.0.7",
    "react-responsive": "9.0.2",
    "react-drag-drop-files": "^2.3.10",
    "@cyntler/react-doc-viewer": "1.10.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "JaganKumar <jagan.kumar@egov.org.in>",
  "keywords": ["digit","egov","dpg","digit-ui","react","components"]
}
