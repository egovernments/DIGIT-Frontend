    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.6.0"
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "**/babel-loader": "8.2.2",
    "**/@babel/core": "7.14.0",
    "**/@babel/preset-env": "7.14.0",
    "**/@babel/plugin-transform-modules-commonjs": "7.14.0",
    "**/polished": "4.2.2",
    "fast-uri": "2.1.0"
  },
  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development",
    "started":"cd packages && yarn build && cd ../ && yarn start:dev",
    "start:core":"cd packages && yarn build:core && cd ../ && yarn start:dev",
    "start:dev": "webpack serve --config webpack.dev.js --mode development",
    "build": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "build:prepare": "./build.sh",
    "build:libraries": "cd packages && yarn build",
    "build:prod": "webpack --mode production",
    "build:webpack": "yarn build:libraries && cd .. && ls && cd ./web && ls && yarn build:prod",
    "clean": "rm -rf node_modules && cd packages && yarn clean"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}