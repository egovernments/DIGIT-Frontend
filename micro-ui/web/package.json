  "scripts": {
    "start": "node start-dev.js",
    "start:dev": "node start-dev.js",
    "start:webpack-only": "webpack serve --config webpack.dev.js --port 3000",
    "dev:packages": "concurrently \"yarn dev:css\" \"yarn dev:modules\"",
    "dev:css": "cd packages/css && yarn start",
    "dev:modules": "run-p dev:birth dev:death dev:firenoc dev:pt dev:tl dev:ws dev:hrms dev:pgr dev:receipts dev:bills dev:engagement dev:finance",
    "dev:birth": "cd packages/modules/birth && yarn build:dev --watch",
    "dev:death": "cd packages/modules/death && yarn build:dev --watch",
    "dev:firenoc": "cd packages/modules/firenoc && yarn build:dev --watch",
    "dev:pt": "cd packages/modules/property-tax && yarn build:dev --watch",
    "dev:tl": "cd packages/modules/tl && yarn build:dev --watch",
    "dev:ws": "cd packages/modules/ws && yarn build:dev --watch",
    "dev:hrms": "cd packages/modules/hrms && yarn build:dev --watch",
    "dev:pgr": "cd packages/modules/pgr && yarn build:dev --watch",
    "dev:receipts": "cd packages/modules/receipts && yarn build:dev --watch",
    "dev:bills": "cd packages/modules/bills && yarn build:dev --watch",
    "dev:engagement": "cd packages/modules/engagement && yarn build:dev --watch",
    "dev:finance": "cd packages/modules/finance && yarn build:dev --watch",
    "build": "npm run build:packages && webpack --config webpack.prod.js",
    "build:prod": "npm run build:packages && webpack --config webpack.prod.js",
    "build:analyze": "npm run build:packages && ANALYZE=true webpack --config webpack.prod.js",
    "build:packages": "run-s build:css build:modules",
    "build:css": "cd packages/css && yarn build",
    "build:modules": "run-s build:birth build:death build:firenoc build:pt build:tl build:ws build:hrms build:pgr build:receipts build:bills build:engagement build:finance",
    "build:birth": "cd packages/modules/birth && yarn build",
    "build:death": "cd packages/modules/death && yarn build",
    "build:firenoc": "cd packages/modules/firenoc && yarn build",
    "build:pt": "cd packages/modules/property-tax && yarn build",
    "build:tl": "cd packages/modules/tl && yarn build",
    "build:ws": "cd packages/modules/ws && yarn build",
    "build:hrms": "cd packages/modules/hrms && yarn build",
    "build:pgr": "cd packages/modules/pgr && yarn build",
    "build:receipts": "cd packages/modules/receipts && yarn build",
    "build:bills": "cd packages/modules/bills && yarn build",
    "build:engagement": "cd packages/modules/engagement && yarn build",
    "build:finance": "cd packages/modules/finance && yarn build",
    "clean": "rm -rf node_modules build dist && find packages -name node_modules -type d -exec rm -rf {} + 2>/dev/null || true",
    "cleancmd": "find . \\( -name 'node_modules' -o -name 'dist' -o -name 'build' \\) -type d -prune -exec rm -rf '{}' +"
  },
