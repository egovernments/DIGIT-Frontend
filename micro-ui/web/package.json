  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "workspaces": [
    "packages/css",
    "packages/modules/core",
    "packages/modules/workbench"
  ],
  "homepage": "/workbench-ui/",
  "husky": {},
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "dotenv-webpack": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "^5.6.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "vite": "^6.1.0",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-merge": "^5.10.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-private-property-in-object": "7.21.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@egovernments/digit-ui-libraries": "2.0.0-rc19-01",
    "@egovernments/digit-ui-module-core": "2.0.0-rc19-03",
    "@egovernments/digit-ui-react-components": "2.0.0-rc19-02",
    "@egovernments/digit-ui-svg-components": "2.0.0-rc19-01",
    "@egovernments/digit-ui-components": "2.0.0-rc19-01",
    "@egovernments/digit-ui-module-workbench": "2.0.0-rc19-04",
    "@tanstack/react-query": "^5.62.16",
    "babel-loader": "^9",
    "cross-env": "^7.0.3",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "5.2.6",
    "http-proxy-middleware": "1.3.1",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "microbundle-crl": "0.13.11",
    "process": "^0.11.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.52.2",
    "react-i18next": "15.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.25.1",
    "style-loader": "2.0.0",
    "terser-brunch": "^4.1.0",
    "url": "^0.11.4",
    "web-vitals": "1.1.2"
  },
  "resolutions": {
    "**/@babel/runtime": "7.20.1",
    "**/babel-preset-react-app": "10.0.0",
    "**/styled-components": "5.0.0",
    "fast-uri": "2.1.0",
    "**/react": "19.0.0",
    "**/react-dom": "19.0.0",
    "**/react-query": "@tanstack/react-query@^5.62.16",
    "**/@tanstack/react-query": "^5.62.16"
  },
  "scripts": {
    "start": "node start-dev.js",
    "start:dev": "node start-dev.js",
    "start:webpack-only": "webpack serve --config webpack.dev.js --port 3000",
    "dev:packages": "concurrently \"npm run dev:css\" \"npm run dev:core\" \"npm run dev:workbench\"",
    "dev:css": "cd packages/css && npm run start",
    "dev:core": "cd packages/modules/core && npm run build:dev -- --watch",
    "dev:workbench": "cd packages/modules/workbench && npm run build:dev -- --watch",
    "build": "npm run build:packages && webpack --config webpack.prod.js",
    "build:prod": "npm run build:packages && webpack --config webpack.prod.js",
    "build:analyze": "npm run build:packages && ANALYZE=true webpack --config webpack.prod.js",
    "build:workbench": "cd packages/modules/workbench && npm run build",
    "build:core": "cd packages/modules/core && npm run build",
    "build:css": "cd packages/css && npm run build",
    "build:packages": "npm run build:css && npm run build:core && npm run build:workbench",
    "clean": "rm -rf node_modules build dist && find packages -name node_modules -type d -exec rm -rf {} + 2>/dev/null || true",
    "cleancmd": "find . \\( -name 'node_modules' -o -name 'dist' -o -name 'build' \\) -type d -prune -exec rm -rf '{}' +"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
