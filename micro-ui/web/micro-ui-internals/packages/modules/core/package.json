    "react-i18next": "15.0.0",
    "react-redux": "7.2.8",
    "react-responsive": "10.0.1",
    "react-tooltip": "4.1.2",
    "redux": "5.0.0",
    "redux-thunk": "2.4.1"
  },
  "sideEffects": false,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
